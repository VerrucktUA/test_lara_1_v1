{
    "name": "phoenix/dns-library",
    "description": "A PHP library for retrieving DNS records for a specified domain name",
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "DnsLibrary\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DnsLibrary\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "test": "phpunit"
    }
}